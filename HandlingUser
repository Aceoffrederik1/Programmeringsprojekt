import java.util.*;
public class HandlingUser
 {
   public ArrayList<User> users;
    
   public HandlingUser(){
       users = new ArrayList<User>();

   }
   
   public void AddNewUser(User user){
       users.add(user);
   }
   
   public void RemoveUser(User user){
       users.remove(user);
   }
   
   public void Print(){
       for(User user: users){
           System.out.println(user.toString());
    }
}
  }
